<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="model.pojo.Department" table="Departments">
    <id name="Id" column="DepartmentsId" type="integer">
        <generator class="native"></generator>
    </id>
    <property name="Department_Name" column="Departments_Name" type="string"></property>
  </class>
</hibernate-mapping>
