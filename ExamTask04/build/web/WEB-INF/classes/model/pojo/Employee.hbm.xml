<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.pojo.Employee" table="Employees">
        <id name="Id" column="EmployeesId" type="integer">
            <generator class="native"></generator>
        </id>
        <property name="Employees_Name" column="Employees_Name" type="string"></property>
        <property name="IdCode" column="IdCode" type="integer"></property>
    </class>
</hibernate-mapping>
